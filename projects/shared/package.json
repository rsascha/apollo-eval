{
  "name": "@apollo-eval/shared",
  "version": "1.0.0",
  "description": "Shared types and utilities for Apollo Eval project",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./database": {
      "import": "./dist/database.js",
      "types": "./dist/database.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "apollo",
    "graphql",
    "shared",
    "types"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "typescript": "~5.8.3"
  },
  "peerDependencies": {},
  "dependencies": {}
}
